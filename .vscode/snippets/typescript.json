{
  // Place your snippets for typescript here. Each snippet is defined under a snippet name and has a prefix, body and
  // description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
  // $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the
  // same ids are connected.
  // Example:
  // "Print to console": {
  // 	"prefix": "log",
  // 	"body": [
  // 		"console.log('$1');",
  // 		"$2"
  // 	],
  // 	"description": "Log output to console"
  // }
  "console1": {
    "prefix": "con",
    "body": "console.log('$1: ', $1)",
    "description": "Log output to console"
  },
  "console2": {
    "prefix": "log",
    "body": "console.log('$1: ', $1)",
    "description": "Log output to console"
  },

  "importNamed": {
    "prefix": "imp",
    "body": "import { ${2:name} } from '${1:module}'$0",
    "description": "Named import"
  },
  "importDefault": {
    "prefix": "imd",
    "body": "import ${2:name} from '${1:module}'$0",
    "description": "Default import"
  },
  "importNamespace": {
    "prefix": "imn",
    "body": "import * as ${2:alias} from '${1:module}'$0",
    "description": "Namespace import"
  },
  "constructor": {
    "prefix": "con",
    "body": "constructor(${1:params}) {\n\t${0}\n}",
    "description": "Constructor"
  },
  "importReact": {
    "prefix": "imr",
    "body": "import * as React from 'react'$0",
    "description": "import react"
  },
  "importQueryString": {
    "prefix": "imq",
    "body": "import * as queryString from 'query-string'$0",
    "description": "import 'query-string'"
  }
}
