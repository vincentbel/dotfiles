{
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.quickSuggestions": true,
    "editor.wordWrap": "on"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "octref.vetur"
  },
  "amVim.smartRelativeLineNumbers": true,
  "amVim.useSystemClipboard": true,
  "breadcrumbs.enabled": false,
  "cSpell.showStatus": false,
  "cSpell.userWords": [
    "adcode",
    "alife",
    "amap",
    "frontmatter",
    "kubernetes",
    "lifecycle",
    "nginx",
    "polyfill"
  ],
  "cssVariables.lookupFiles": [
    "node_modules/@alife/dt-oui-style/es/theme/default.css"
  ],
  "editor.fontFamily": "'Comic Mono', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 15,
  "editor.formatOnPaste": false,
  "editor.formatOnSave": false,
  "editor.gotoLocation.multipleDeclarations": "gotoAndPeek",
  "editor.gotoLocation.multipleDefinitions": "gotoAndPeek",
  "editor.inlineSuggest.enabled": true,
  "editor.snippetSuggestions": "top",
  "editor.suggest.preview": true,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "eslint.migration.2_x": "off",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.autoUpdate": "onlyEnabledExtensions",
  "files.autoSave": "onFocusChange",
  "files.defaultLanguage": "markdown",
  "files.insertFinalNewline": true,
  "git.autoStash": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "gitlens.advanced.fileHistoryFollowsRenames": true,
  "gitlens.blame.dateFormat": "YYYY-MM-DD",
  "gitlens.blame.format": "${author|8} ${message|40} ${date|10-}",
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.defaultDateFormat": "YYYY-MM-DD HH:mm:ss",
  "gitlens.defaultDateShortFormat": "YYYY-MM-DD",
  "gitlens.fileAnnotations.command": "blame",
  "gitlens.gitCommands.closeOnFocusOut": true,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.statusBar.enabled": false,
  "gitlens.views.repositories.branches.layout": "list",
  "gitlens.views.repositories.files.layout": "tree",
  "html-css-class-completion.excludeGlobPattern": "**/node_modules",
  "html-css-class-completion.includeGlobPattern": "**/*.{css,scss}",
  "javascript.preferences.useAliasesForRenames": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "local-history.path": "~/dev/dotfiles/.vscode/",
  "macros": {
    "commentAndCursorDown": [
      "editor.action.commentLine",
      "cursorDown"
    ]
  },
  "material-icon-theme.folders.color": "#78909c",
  "material-icon-theme.folders.theme": "classic",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "pasteImage.filePathConfirmInputBoxMode": "onlyName",
  "pasteImage.path": "${currentFileDir}/images",
  "pasteImage.showFilePathConfirmInputBox": true,
  "polacode.transparentBackground": true,
  "prettier.singleQuote": true,
  "python.defaultInterpreterPath": "/usr/bin/python3",
  "python.formatting.provider": "yapf",
  "python.languageServer": "Pylance",
  "python.pythonPath": "/usr/bin/python3",
  "scm.defaultViewMode": "tree",
  "search.followSymlinks": false,
  "search.searchEditor.defaultNumberOfContextLines": 2,
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.scrollback": 10000,
  "terminal.integrated.tabs.enabled": true,
  "terminal.integrated.tabs.location": "left",
  "todo-tree.general.statusBar": "top three",
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]"
  ],
  "todo-tree.highlights.customHighlight": {
    "FIXME": {
      "background": "#f06292",
      "foreground": "white",
      "iconColour": "#f06292",
      "opacity": 90
    },
    "TODO": {
      "background": "yellow",
      "foreground": "black",
      "iconColour": "yellow",
      "opacity": 90
    }
  },
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "typescript.disableAutomaticTypeAcquisition": true,
  "typescript.preferences.useAliasesForRenames": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vim.easymotion": true,
  "vim.leader": " ",
  "vim.otherModesKeyBindingsNonRecursive": [
    {
      "after": [
        "g",
        "j"
      ],
      "before": [
        "j"
      ]
    },
    {
      "after": [
        "g",
        "k"
      ],
      "before": [
        "k"
      ]
    },
    {
      "after": [],
      "before": [
        "<leader>",
        "g",
        "o"
      ],
      "commands": [
        {
          "args": [],
          "command": "extension.openInGitHub"
        }
      ]
    },
    {
      "after": [],
      "before": [
        "<leader>",
        "g",
        "d"
      ],
      "commands": [
        {
          "args": [],
          "command": "git.openChange"
        }
      ]
    },
    {
      "after": [],
      "before": [
        "<leader>",
        "l",
        "h"
      ],
      "commands": [
        {
          "args": [],
          "command": "local-history.compareToActive"
        }
      ]
    },
    {
      "after": [],
      "before": [
        "<leader>",
        "f",
        "d"
      ],
      "commands": [
        {
          "args": [],
          "command": "editor.action.formatDocument"
        }
      ]
    },
    {
      "after": [],
      "before": [
        "<leader>",
        "f",
        "s"
      ],
      "commands": [
        {
          "args": [],
          "command": "editor.action.formatSelection"
        }
      ]
    },
    {
      "after": [],
      // lint next
      "before": [
        "<leader>",
        "l",
        "n"
      ],
      "commands": [
        {
          "args": [],
          "command": "editor.action.marker.next"
        }
      ]
    },
    {
      "after": [],
      // rename symbol
      "before": [
        "<leader>",
        "r",
        "s"
      ],
      "commands": [
        {
          "args": [],
          "command": "editor.action.rename"
        }
      ]
    },
    {
      "after": [],
      // open markdown preview
      "before": [
        "<leader>",
        "m",
        "p"
      ],
      "commands": [
        {
          "args": [],
          "command": "markdown.showPreviewToSide"
        }
      ]
    }
  ],
  "vim.overrideCopy": false,
  "vim.useSystemClipboard": true,
  "vscode-neovim.neovimExecutablePaths.darwin": "/usr/local/bin/nvim",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "window.title": "${rootName}${separator}${activeEditorMedium}",
  "window.titleSeparator": " —— ",
  "window.zoomLevel": 1,
  "workbench.activityBar.visible": true,
  "workbench.colorCustomizations": {
    "editor.findMatchBackground": "#ff953271",
    "editor.findMatchBorder": "#ff9632",
    "editor.findMatchHighlightBackground": "#ffff0059",
    "editor.findMatchHighlightBorder": "#ffff0096",
    "minimapSlider.activeBackground": "#ffffff62",
    "minimapSlider.background": "#ffffff2b",
    "minimapSlider.hoverBackground": "#ffffff58",
    // https://github.com/Tyriar/vscode-snazzy
    // The background is optional
    // "terminal.background": "#282a36",
    "terminal.ansiBlack": "#282a36",
    "terminal.ansiBlue": "#57c7ff",
    "terminal.ansiBrightBlack": "#686868",
    "terminal.ansiBrightBlue": "#57c7ff",
    "terminal.ansiBrightCyan": "#9aedfe",
    "terminal.ansiBrightGreen": "#5af78e",
    "terminal.ansiBrightMagenta": "#ff6ac1",
    "terminal.ansiBrightRed": "#ff5c57",
    "terminal.ansiBrightWhite": "#eff0eb",
    "terminal.ansiBrightYellow": "#f3f99d",
    "terminal.ansiCyan": "#9aedfe",
    "terminal.ansiGreen": "#5af78e",
    "terminal.ansiMagenta": "#ff6ac1",
    "terminal.ansiRed": "#ff5c57",
    "terminal.ansiWhite": "#f1f1f0",
    "terminal.ansiYellow": "#f3f99d",
    "terminal.foreground": "#eff0eb",
    "terminal.selectionBackground": "#ffdb1081"
  },
  "workbench.colorTheme": "GitHub Dark Dimmed",
  "workbench.editor.decorations.colors": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "workbench.iconTheme": "material-icon-theme",
  "workbench.list.automaticKeyboardNavigation": false,
  "workbench.list.horizontalScrolling": true,
  "workbench.settings.editor": "json",
  "workbench.settings.useSplitJSON": true,
  "zenMode.fullScreen": false
}
